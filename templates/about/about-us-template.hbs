<h1>About Us</h1>
{{#each data}}
  <div>
    <img
      src="{{thumbpath}}"
      alt="{{name}}"
      class="float-start rounded-4 me-3"
    />
    <p class="text-black-50">
      {{{summary}}}
    </p>
    <a
      href="#aboutus/{{id}}"
      class="btn btn-link readmore"
      onclick="loadDetails('aboutus/{{id}}','details-template','content')"
    >readmore</a>
  </div>
{{/each}}