<h1>Categories</h1>
<!-- vung chua danh sach cataloge -->
<ul class="py-3">
  {{#each data}}
    <li>
      <a
        href="#blogs/categories/{{id}}"
        class="text-black-50 text-decoration-none"
        onclick="loadBlogsPage('blogs/categories/{{id}}')"
      >{{name}}</a>
    </li>
  {{/each}}
</ul>